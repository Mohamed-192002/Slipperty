@model OrderHeadDTO

<div id="card-pagintation" class="row align-items-center mb-3 " >
    <h1 class="newOrderHeader d-flex justify-content-between align-items-center">
        <div class="d-flex gap-2 align-items-center">
            @ViewBag.PageSize / <span>@ViewBag.PageNumber</span>
        </div>
        @if (Model.StatusId != (int)ActionType.New)
        {
            <div>
                <button class="btn btn-secondary dropdown-toggle"  id="dropdownMenuButton1" data-bs-toggle="dropdown" aria-expanded="false" style="width: 62px; height: 34px; border-radius: 10px ; background-color: white; border: 1px solid var(--primary-color); color:var(--primary-color)">
                    <p id="HoursSinceLastTransaction">@(
                                                      Model?.OrderCardFilter?.HoursSinceLastTransaction is not null  
                                                          ? Model.OrderCardFilter.HoursSinceLastTransaction.Value.ToString() 
                                                          : "اختر"
                                                      )
                        </p><i class="fa-solid fa-chevron-down"></i>
                </button>
                <ul class="dropdown-menu" aria-labelledby="dropdownMenuButton1" style="border-radius: 10px; min-width:3rem">
                    <li><a class="dropdown-item " style="cursor: pointer"  onclick="$('#dropdownMenuButton1 p').html('24')">24</a></li>
                    <li><a class="dropdown-item " style="cursor: pointer" onclick="$('#dropdownMenuButton1 p').html('48')" >48</a></li>
                    <li><a class="dropdown-item " style="cursor: pointer" onclick="$('#dropdownMenuButton1 p').html('72')">72</a></li>
                </ul>
            </div>
        }
    </h1>
    <div class="col text-end d-flex; gap-4 filterSection">
        <button class="searchButton" id="searchSectionToggelBtn"  (click)="$('#searchForm').toggel()">بحث</button>
        <div class="d-flex justify-content-center align-items-center OrderBy" id="OrderBySection">
            <button class="newButton @(Model?.OrderCardFilter?.SortType?.ToUpper() == "DESC" ? "active" : "")" id="orderDESC">احدث</button>
            <button class="oldButton @(Model?.OrderCardFilter?.SortType?.ToUpper() == "ASC" ? "active" : "") " id="orderASC" >اقدم</button>
        </div>
    </div>
    <div class="col pagination-arrow text-start d-flex justify-content-end align-items-center list-none">
        <li  style="width: 44px ; height: 38px ; border-radius: 10px"   >
            <button class="page-link next-order" @(ViewBag.PageNumber == ViewBag.PageSize ? "disabled" : "")  > < </button>
        </li>

        <li  style="width: 44px ; height: 38px ; border-radius: 10px"  >
            <button class="page-link prev-order" @(ViewBag.PageNumber == 1 ? "disabled" : "") > > </button>
        </li>
    </div>
</div>
