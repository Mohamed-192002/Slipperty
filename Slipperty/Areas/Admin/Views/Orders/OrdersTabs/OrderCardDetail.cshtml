<div>
  @await Html.PartialAsync("OrdersTabs/OrderCardFilterSection", Model.OrderCardFilter)
</div>
<div class="card shadow-md col-lg-12 col-md-12 col-sm-12 position-relative order-card"
     style="background-color: #FFFFFF; box-shadow: 0px 4px 10px rgba(0, 0, 0, 0.25); border-radius: 10px;" order-id="@Model.Id">
    <div class="card-body text-end">
        @if (@Model.StatusId != (int)ActionType.New && @Model?.FollowUpDetails?.Count() > 0)
        {
                    <div>
                        <div class="d-flex align-items-center gap-4">
                            <h2 style="color: var(--primary-color);
                           font-weight: bolder;
                           margin: 0px;">عدد المحاولات</h2>
                            <span style="
                width: 39px;
                height: 37px;
                color: #FFFFFF;
                background-color: rgba(93, 36, 225, 1);
                display: flex;
                justify-content: center;
                align-items: center;
                border-radius: 16px;
                padding: 4px;
                font-size: 20px;
                font-weight: 700;">@Model?.FollowUpDetails?.Count()</span>
                        </div>
                    @foreach (var followup in Model.FollowUpDetails)
                    {
                        
                        <div>
                            <p class="mb-0">
                                @await Html.PartialAsync("OrdersTabs/Modal/FolloUpStatus", followup.ContactStatusId??0)
                                @{
                                    var createdDate = (DateTime)followup.CreatedOn;
                                    var now = DateTime.Now;
                                    var timeSpan = now - createdDate;
                                    var days = timeSpan.Days;
                                    var hours = timeSpan.Hours;
                                }
                                <span>منذ @($"{days} يوم و {hours} ساعة")</span>
                            </p>
                            <div class="alert alert-primary" style="
                                                                width: fit-content;
                                                                height: 22px;
                                                                border-radius: 6px;
                                                                display: flex;
                                                                justify-content: center;
                                                                align-items: center;
                                                                color: white;
                                                                background: rgba(93, 36, 225, 0.7);
                                                                margin-bottom: 0;" role="alert">
                                @ContactStatusHelper.GetContactStatus(followup.ContactStatusId??0)
                            </div>
                            <div class="alert alert-primary" style="color:black;background-color: rgba(93, 36, 225, 0.3)" role="alert">
                                @followup.Notes
                            </div>
                        </div>
                    }    
                    </div>            
        }


        <div class="d-flex justify-content-between align-items-center">
            <h5 class="card-title d-flex align-items-center gap-2">
                <span id="orderNoCopiedValue">@Model?.orderNo</span> <i class="fa-regular fa-copy" id="copy-orderNo" style="color: var(--primary-color)" aria-hidden="true"></i>
                <span id="tooltip" class="tooltip-text">تم نسخ رقم الطلب بنجاح.</span>

            </h5>
            <div>
            @if (@Model?.StatusId == (int?)ActionType.Confirmed && @Model?.OrderUrgentDetails != null)
            {
                
                   <h6>طلب مستعجل <img src="~/img/hot.png"/></h6>
                    
              
            }
            @if (Model?.StatusId == (int?)ActionType.Holding && Model.OrderHoldingDetails != null)
            {
                var holdingDate = Model.OrderHoldingDetails.Last().HoldingDate;
                var now = DateTime.Now;
                var timeSpan = holdingDate  - now  ;

                var days = timeSpan.Days;
                var hours = timeSpan.Hours;
                <div>

                    <h6>باقي @($"{days} يوم و {hours} ساعة")</h6>
                </div>


            }
            @if (Model?.StatusId != (int)ActionType.New && Model?.OrderModificationDeclined != null)
            {
                <h6>قام هذا العميل برفض التعديل على الطلب</h6>

            }
            </div>
        </div>
        <h6 class="card-subtitle mb-2">
            @{
                if (Model?.RegDate != null)
                {
                    var regDate = Model.RegDate.Date;
                    var now = DateTime.Now;
                    var timeSpan = now - regDate;

                    var days = timeSpan.Days;
                    var hours = timeSpan.Hours;

                    @($"{regDate:dd/MM/yyyy} - منذ {days} يوم و {hours} ساعة")
                }
            }
        </h6>
        <div class="row flex-wrap-reverse">
            <div class="col-md-6 text-end">
                <h6>@Model?.fullName</h6>
                <h6>@Model?.Government?.Name</h6>
                <h6>@Model?.Region?.Name</h6>
                <div class="d-flex align-items-baseline gap-2">
                    <p class="m-0">@Model?.Address</p>
                    <div class="icons d-flex align-items-center gap-2" style="color:var(--primary-color)">
                        @* <i class="fa-regular fa-square-plus cursor-pointer" ></i> *@
                        <i class="fa-regular fa-pen-to-square cursor-pointer" id="modify-order-detail-modal"></i>
                    </div>
                </div>
                @if (!string.IsNullOrEmpty(Model?.phoneNumber))
                {
                    <a class="card-text d-flex align-items-center gap-2 cursor-pointer" style="color: var(--primary-color); width : fit-content; " href="tel:@Model.phoneNumber">
                        <i class="fa-solid fa-phone-flip"></i> @(Model.phoneNumber?.StartsWith("2") == true ? Model.phoneNumber.Substring(1) : Model.phoneNumber)
                    </a>
                }
                @if (!string.IsNullOrEmpty(Model?.phoneNumber2))
                {
                    <a class="card-text d-flex align-items-center gap-2 cursor-pointer" style="color: var(--primary-color); width : fit-content; " href="tel:@Model.phoneNumber2">
                        <i class="fa-solid fa-phone-flip"></i> @(Model.phoneNumber2?.StartsWith("2") == true ? Model.phoneNumber2.Substring(1) : Model.phoneNumber2)
                    </a>
                }
                <p class="card-text">
                    @{
                        string fromTime = Model?.deliveryTimeFrom?.Replace("ص", "صباحآ").Replace("م", "مساءآ");
                        string toTime = Model?.deliveryTimeTo?.Replace("ص", "صباحآ").Replace("م", "مساءآ");
                    }
                    @(fromTime) | @(toTime)
                </p>

                <p class="d-flex gap-2">
                    <span class="text-muted">الإجمالي:</span>
                    <span>
                        <span id="netAmountValue">
                            @if ((int)ActionType.Confirmed == Model.StatusId)
                            {
                                @Math.Floor((double)(Model.NetAmount + ((decimal?)Model.OrderUrgentDetails?.AdditionalPrice ?? 0m)))
                            }
                            else if ((int)ActionType.Holding == Model.StatusId)
                            {
                                @Math.Floor((double)(Model.NetAmount + ((decimal?)Model.OrderHoldingDetails?.Last().AdditionalPrice ?? 0m)))
                            }
                            else
                            {
                                @Math.Floor((double)(Model.NetAmount))
                            }
                        </span> جنية مصري</span>
                </p>

                <p class="card-text">
                    <span style="color: var(--primary-color)">الملاحظات</span>: @Model?.Notes
                </p>
            </div>

            <div class="col-md-6">
                <div style="color: var(--primary-color)">
                    <i class="fa-solid fa-square-plus" data-bs-toggle="tooltip" title="اضافة منتج" id="add-product-icon"></i>
                </div>
                <div class="p-2 order-product-wrapper">
                    @foreach (var detail in Model?.OrderDetails)
                    {
                        <div class="card mt-2 position-relative"
                             style="background-color: var(--primary-color); color: white; border-radius: 10px;">
                            <i class="position-absolute start-0 top-0 p-3 btn-group dropend">
                                <img src="~/images/icons/0c5c711b01d1606177f38c367cc76918.png" data-bs-toggle="dropdown" data-bs-display="static" aria-expanded="false" id="orderDetail-@detail.Id" alt="Icon" style="width: 16px; height: 16px"/>
                                <ul class="dropdown-menu dropdown-menu-lg-end text-end">
                                    <li><button class="dropdown-item border-bottom order-detail-product" type="button" data-action-url="Admin/Orders/GetOrdersDetailProduct?ActionMod=Edit&orderDetailId=@detail.Id">تعديل المنتج</button></li>
                                    <li><button class="dropdown-item order-detail-product" type="button" data-action-url="Admin/Orders/GetOrdersDetailProduct?ActionMod=Change&orderDetailId=@detail.Id">تغير المنتج</button></li>
                                    <li><button class="dropdown-item order-detail-product" type="button" data-action-url="Admin/Orders/GetOrdersDetailProduct?ActionMod=Delete&orderDetailId=@detail.Id">حذف المنتج</button></li>
                                </ul>
                            </i>
                            <div class="card-body row text-white">
                                <div class="col-6 text-end">
                                    <h5>@detail?.Product?.EngName</h5>
                                    <div>
                                        @detail?.Product?.EngName.Split(" ")[0]  
                                        @foreach (var res in detail?.Combination?.Text?.Split(","))
                                        {
                                            @res.Split(":")[1] 
                                            <span> </span>
                                        }
                                    </div>
                                    <h5>@(detail?.Qty) الكمية</h5>
                                    <h5>@Math.Floor((detail?.Product?.DiscountPrice ?? 0) != 0
                                            ? detail?.Product?.DiscountPrice ?? 0
                                            : detail?.Product?.Price ?? 0) جنيه</h5>
                                </div>
                                <div class="col-6 text-center d-flex justify-content-center align-items-center">
                                    <img src="@Url.Content($"~/{ImagesPathes.Products}{detail?.Product?.MainImageUrl}")" alt="Product Image"
                                         class="img-fluid rounded-3"
                                         style="max-width: 100px; height: auto;">
                                </div>
                            </div>
                        </div>
                    }
                </div>
            </div>
        </div>
    </div>
    @await Html.PartialAsync("OrdersTabs/Modal/ActionsModel", Model?.StatusId)
    @await Html.PartialAsync("OrdersTabs/Modal/UrgentModal")
    @await Html.PartialAsync("OrdersTabs/Modal/PostponementModal")
    @await Html.PartialAsync("OrdersTabs/Modal/CancelModal")
    @await Html.PartialAsync("OrdersTabs/Modal/FolloUpModal")
    @await Html.PartialAsync("OrdersTabs/Modal/AddUserPhoneModal")
    @await Html.PartialAsync("OrdersTabs/Modal/EditUserAddressModal" , Model?.Address)
    
</div>

@{

        <div class="d-flex justify-content-center align-items-center gap-1">
            <a class="btn btn-primary mt-5 order-btn" ActionType="@Convert.ToInt32(ActionType.Confirmed)">تأكيد</a>
            <button type="button" class="btn btn-primary mt-5 order-btn" data-toggle="modal" data-target="#exampleModalCenter" onclick="$('#ActionModal').modal('show')">متابعة</button>
        </div>
}


<script>
    
    var TransfareData = {
            OrderId : @Model.Id, 
            status : getTabStatus($(".tab-color.active").attr("id"))
        }
    
    
    function GetOrderCardDetail (){
        let OrderId = @Model.Id;
        let status = getTabStatus($(".tab-color.active").attr("id"));
        let url = `@Url.Action("GetOrderCardDetail", "Orders")?OrderId=${OrderId} &status=${status}`;
        $("#partial-container").html(`
                                                <div class="text-center mt-5">
                                                    <div class="spinner-border" style="color:var(--primary-color)" role="status">
                                                        <span class="visually-hidden">Loading...</span>
                                                    </div>
                                                </div>
                                            `);
        $.get(url, function (data) {
            $("#partial-container").html(data);
        }).fail(function () {
            $("#partial-container").html('<p class="text-center text-danger">حدث خطأ أثناء تحميل البيانات.</p>');
        });
    }
     $(document).ready(() => {
  

         
         $(".order-detail-product").on("click" , (event) => {
             openOrderDetailModifiedProductModal($(event.target).data('action-url')); 
         }); 
           
         $("a").on("click" , (event) => {
             let ActionType  = parseInt(event.currentTarget.getAttribute("ActionType"));
             let modalId = event.currentTarget.getAttribute("modal-id");
             let modalDataContainerId = event.currentTarget.getAttribute("modal-data-container-id");
              switch (ActionType){
                 case @Convert.ToInt32(ActionType.Confirmed):
                       sendOrderAction(@Model.Id , ActionType ,  null , modalId, loadTabContent)
                      break;
                  case @Convert.ToInt32(ActionType.FollowUps):
                       sendOrderAction(@Model.Id , ActionType ,  FolloUp(modalDataContainerId) , modalId, loadTabContent)
                       break;
                  case @Convert.ToInt32(ActionType.Cancel) :
                       sendOrderAction(@Model.Id , ActionType ,  CancelModal(modalDataContainerId), modalId, loadTabContent)
                       break;
                  case @Convert.ToInt32(ActionType.Holding):
                       sendOrderAction(@Model.Id , ActionType ,  Postponement(modalDataContainerId), modalId, loadTabContent)
                       break;
                  case @Convert.ToInt32(ActionType.Urgent):
                      sendOrderAction(@Model.Id , ActionType ,  Urget(modalDataContainerId), modalId, loadTabContent)
                       break;
                  case @Convert.ToInt32(ActionType.Modified):
                      sendOrderAction(@Model.Id , ActionType ,  OrderModificationData(modalDataContainerId), modalId, GetOrderCardDetail)
                      break;
                  case @Convert.ToInt32(ActionType.StoppedConfirmed):
                        sendOrderAction(@Model.Id , ActionType , {} , modalId , loadTabContent);
                        break;
                   default : 
                        console.log("error not impelmeneted method");     
               }
         })

        function sendOrderAction(orderId, actionType, requestData, modalId, loadCallBackFunction = () => {}) {
        
            $.ajax({
                url: `/OrderAction/${orderId}/${actionType}`, 
                type: 'POST',                                
                contentType: 'application/json',             
                data: JSON.stringify(requestData),           
                success: function(response) {
              
                    toastr.options = {
                        positionClass: 'toast-top-right',
                    };
                    toastr.success("تمت العملية بنحاج");
                  
                    $(`#${modalId}`).modal('hide') ;
                    loadCallBackFunction();
                },
                error: function(xhr, status, error) {
                    toastr.options = {
                        positionClass: 'toast-top-right'
                    };
                    toastr.error("حاول مرة اخري");
                    console.error("Error:", xhr.responseText); 
                }
            });
        }



         function loadTabContent() {
             let status = getTabStatus($(".tab-color.active").attr("id"))
             let url = `@Url.Action("GetOrders", "Orders")?status=${status}`;
             $("#partial-container").html(`
                                                <div class="text-center mt-5">
                                                    <div class="spinner-border" style="color:var(--primary-color)" role="status">
                                                        <span class="visually-hidden">Loading...</span>
                                                    </div>
                                                </div>
                                            `);
             $.get(url, function (data) {
                 $("#partial-container").html(data);
             }).fail(function () {
                 $("#partial-container").html('<p class="text-center text-danger fs-3 fw-bolder">حدث خطأ أثناء تحميل البيانات.</p>');
             });
         }
               
         
         function Urget(modalId){
            let inputs = $(`#${modalId} li input`);
            return {
                UrgentDate : inputs[0].value,
                UrgentAdditionalPrice : inputs[1].value
            }
         }

         function Postponement(modalId){
             let inputs = $(`#${modalId} li input`); 
             return {
                 PostponementDate : inputs[0].value,
                 ShipmentPrice : inputs[1].value
             }
         }


         function CancelModal(modalId) {
                let activeItem = $(`#${modalId} li.active`);
                if (activeItem.length === 0) {
                    console.log("No active item selected");
                    return;
                }
                let index = activeItem.index();
                let notes = activeItem.find('input').val(); 
                return {
                    Notes: notes,
                    Id: index + 1
                };
            }
         

         function FolloUp(modalId){
             if (modalId[0] === "*"){
              let obj =  
                 {
                     ContactMethodId : 2, 
                     OrderId : @Model.Id , 
                     ContactStatusId :  1, 
                     FollowUpNote : ""
                 };  
                 return obj; 
             } 
            
            let activeItem = $("#"+modalId + " li.active");
            if (activeItem.length === 0) {
                return;
            }
            let index = activeItem.index();
            let notes = activeItem.find('input').val(); 
            return {
                FollowUpNote: notes,
                Id: index + 1
            };
         }
         
         function OrderModificationData(modalId){
             let modifiedObj = {};
             $(`#${modalId} li [name]`).each((index, element) => {
                 modifiedObj[$(element).attr("name")] =  $(element).val();
             });
            // console.log(modifiedObj); 
             return modifiedObj;
         }
         
         function openOrderDetailModifiedProductModal(ActionUrl)
         {
            
             $("#loader").removeClass("d-none");
             $("#ajaxModal").html();
             $.get(ActionUrl, function (data) {
                 $("#ajaxModal").html(data);
                 $("#OrderDetailModifyProductModal").modal('show');
                 $("#loader").addClass("d-none");
             }).fail(function (error) {
                 toastr.options = {
                     positionClass: 'toast-top-right'
                 };
                 toastr.error("حاول مرة اخري");
                 console.error("Error:", error);
                 $("#ajaxModal").html('');
                 $("#loader").addClass("d-none");
             });
         }
         


     })

</script>
